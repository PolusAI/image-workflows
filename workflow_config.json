{
    "BBBC001": {
        "name": "BBBC001",
        "size": "very small",
        "subdirectories": ["human_ht29_colon_cancer_1_images"],
        "config":{
            "BbbcDownload": {
                "name": "BBBC001"
            },
            "FileRenaming": {
                "filePattern": ".*_{row:c}{col:dd}f{f:dd}d{channel:d}.tif",
                "outFilePattern": "x{row:dd}_y{col:dd}_p{f:dd}_c{channel:d}.tif",
                "mapDirectory": "raw",
                "inpDir": "BbbcDownload.outDir"
            },
            "OmeConverter": {
                "filePattern": ".*.tif",
                "fileExtension": ".ome.tif",
                "inpDir": "FileRenaming.outDir"
            },
            "Montage": {
                "filePattern": "human_ht29_colon_cancer_1_images_x00_y03_p{p:dd}_c0.ome.tif",
                "layout": "p",
                "inpDir": "OmeConverter.outDir"
            },
            "ImageAssembler": {
                "timesliceNaming": "False",
                "imgPath": "OmeConverter.outDir",
                "stitchPath" : "Montage.outDir"
            },
            "PrecomputeSlide": {
                "pyramidType": "Zarr",
                "imageType": "image",
                "inpDir": "ImageAssembler.outDir"
            }
        }
    }
}